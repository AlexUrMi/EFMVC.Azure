<?xml version="1.0" encoding="utf-8"?>
<RoleModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="EFMVC.Web" version="1.7.30602.1703" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <Startup>
    <Task commandLine="DiagnosticsAgent.exe" taskType="background" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
    <Task commandLine="DiagnosticsAgent.exe /blockStartup" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
    <Task commandLine="DotNetUpdate.cmd" executionContext="elevated" relativePath="plugins\Caching">
      <Environment>
        <Variable name="IsEmulated">
          <RoleInstanceValue xpath="/RoleEnvironment/Deployment/@emulated" />
        </Variable>
        <Variable name="CachingLocalStorePath">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/LocalResources/LocalResource[@name='Microsoft.WindowsAzure.Plugins.Caching.FileStore']/@path" />
        </Variable>
        <Variable name="RdModuleName" value="Caching" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
    <Task commandLine="CacheInstaller.exe" executionContext="elevated" relativePath="plugins\Caching">
      <Environment>
        <Variable name="RdModuleName" value="Caching" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
  </Startup>
  <Runtime>
    <EntryPoint>
      <NetFxEntryPoint assemblyName="EFMVC.Web.dll" targetFrameworkVersion="v3.5" />
    </EntryPoint>
  </Runtime>
  <ConfigurationSettings>
    <Setting name="ConnectionString" nameSpace="Microsoft.WindowsAzure.Plugins.Diagnostics" />
    <Setting name="NamedCaches" nameSpace="Microsoft.WindowsAzure.Plugins.Caching" />
    <Setting name="Loglevel" nameSpace="Microsoft.WindowsAzure.Plugins.Caching" />
    <Setting name="CacheSizePercentage" nameSpace="Microsoft.WindowsAzure.Plugins.Caching" />
    <Setting name="ConfigStoreConnectionString" nameSpace="Microsoft.WindowsAzure.Plugins.Caching" />
  </ConfigurationSettings>
  <Properties>
    <Property name="TargetFrameworkVersion" value="v4.0" />
    <Property name="RoleType" value="Web" />
    <Property name="RoleName" value="EFMVC.Web" />
    <Property name="ProjectName" value="EFMVC.Web" />
    <Property name="Configuration" value="Debug" />
    <Property name="Platform" value="AnyCPU" />
    <Property name="RoleTargetFramework" value="v4.0" />
    <Property name="OutputDir" value="D:\GitHub\EFMVC.Azure\EFMVC.Azure\obj\Debug\EFMVC.Web\" />
    <Property name="EntryPoint" value="EFMVC.Web.dll" />
    <Property name="SourceDir" value="D:\GitHub\EFMVC.Azure\EFMVC.Web\" />
    <Property name="SitePhysicalPath" value="D:\GitHub\EFMVC.Azure\EFMVC.Web\" />
    <Property name="TargetRuntimeVersion" value="v4.0" />
  </Properties>
  <Sites>
    <Site name="Web" physicalDirectory="D:\GitHub\EFMVC.Azure\EFMVC.Web\">
      <Bindings>
        <Binding name="Endpoint1" endpointName="Endpoint1" />
      </Bindings>
    </Site>
  </Sites>
</RoleModel>